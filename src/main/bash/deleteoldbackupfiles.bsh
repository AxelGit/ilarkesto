#!/bin/bash

if [[ $# -lt 2 ]]
then
	echo " "
	echo "$0: Illegal arguments."
	echo " "
	echo "Syntax:"
	echo "$0 path deleteolderthenminutes"
	exit 1
fi

DST=$1
CLEANOLDERTHENMINUTES=$2

find "${DST}" -name "*_20*.tbz" -mmin +$CLEANOLDERTHENMINUTES -exec rm {} \;
find "${DST}" -name "*-data_20*.zip" -mmin +$CLEANOLDERTHENMINUTES -exec rm {} \;
